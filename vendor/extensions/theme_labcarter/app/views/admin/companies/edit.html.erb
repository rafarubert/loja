<%= error_messages_for :company %>
<h1><%= t("editing_company") %></h1>

<% form_for(:company, :url => object_url, :html => { :method => :put }) do |f| %>
  <%= render :partial => "form", :locals => { :f => f } %>

    <p class="form-buttons">
      <%= button t("update") %> <%= link_to t('cancel'), collection_url %>
    </p>
<% end %>
